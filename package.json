{
  "name": "clickstyle-web",
  "description": "clickstyle.com.au web/server app using meteor + react + semantic ",
  "version": "0.1.0",
  "engines": {
    "node": "10.0.0",
    "npm": "6.0.1"
  },
  "private": true,
  "author": "Chris Chen",
  "contributors": [
    "Chris Chen <chrischen79@gmail.com>"
  ],
  "scripts": {
    "dev": "MONGO_URL=mongodb://localhost:27017/clickstyle meteor -s settings-development.json",
    "debug": "MONGO_URL=mongodb://localhost:27017/clickstyle meteor run --inspect -s settings-development.json",
    "start": "meteor run",
    "test": "meteor test --driver-package=practicalmeteor:mocha --port 3001",
    "cloc": "cloc imports client server private --exclude-ext=json",
    "predeploy-staging": "heroku config:add METEOR_SETTINGS=\"$(cat settings-staging.json)\" --remote heroku-staging",
    "deploy-staging": "git push heroku-staging develop:master",
    "predeploy-product": "heroku config:add METEOR_SETTINGS=\"$(cat settings-product.json)\" --remote heroku-product",
    "deploy-product": "git push heroku-product develop:master"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.44",
    "@navjobs/upload": "^3.1.3",
    "babel-runtime": "^6.26.0",
    "bcrypt": "^2.0.1",
    "classnames": "^2.2.6",
    "cloudinary": "^1.11.0",
    "commonmark": "^0.28.1",
    "handlebars": "^4.0.10",
    "juice": "^4.3.1",
    "lodash": "^4.17.10",
    "mailchimp-api-v3": "^1.12.0",
    "meteor-node-stubs": "^0.4.1",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.17",
    "prop-types": "^15.6.1",
    "query-string": "^6.1.0",
    "react": "^16.4.1",
    "react-async-script-loader": "^0.3.0",
    "react-avatar-editor": "^11.0.3",
    "react-burger-menu": "^2.5.2",
    "react-collapse": "^4.0.3",
    "react-day-picker": "^7.1.9",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.2.0",
    "react-images": "^0.5.17",
    "react-loading": "^2.0.2",
    "react-motion": "^0.5.2",
    "react-redux": "^5.0.7",
    "react-redux-loading-bar": "^4.0.5",
    "react-router-dom": "^4.3.1",
    "react-share": "^2.2.0",
    "react-slick": "^0.23.1",
    "react-stickynode": "^2.0.1",
    "react-stripe-elements": "^2.0.0",
    "react-typist": "^2.0.4",
    "redux": "^4.0.0",
    "semantic-ui-react": "^0.81.1",
    "simpl-schema": "^1.5.0",
    "stripe": "^6.1.1",
    "uuid": "^3.2.1",
    "validator": "^10.3.0",
    "winston": "^2.4.3"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^3.9.4",
    "gulp-concat": "^2.6.1",
    "gulp-git": "^2.7.0",
    "gulp-s3-upload": "^1.7.0"
  }
}
