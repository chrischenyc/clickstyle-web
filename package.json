{
  "name": "quickstyle-web",
  "version": "0.1.0",
  "description": "quickstyle.com.au web/server app using meteor + react + semantic ",
  "private": true,
  "author": "Chris Chen",
  "contributors": [
    "Chris Chen <chrischen79@gmail.com>"
  ],
  "scripts": {
    "dev": "MONGO_URL=mongodb://localhost:27017/quickstyle meteor -s settings-development.json",
    "debug": "MONGO_URL=mongodb://localhost:27017/quickstyle meteor run --inspect -s settings-development.json",
    "start": "meteor run",
    "test": "meteor test --driver-package=practicalmeteor:mocha --port 3001",
    "cloc": "cloc imports client server private --exclude-ext=json",
    "predeploy-staging": "heroku config:add METEOR_SETTINGS=\"$(cat settings-staging.json)\" --remote heroku-staging",
    "deploy-staging": "git push heroku-staging develop:master",
    "predeploy-product": "heroku config:add METEOR_SETTINGS=\"$(cat settings-product.json)\" --remote heroku-product",
    "deploy-product": "git push heroku-product develop:master"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.44",
    "@navjobs/upload": "^3.1.3",
    "babel-runtime": "^6.26.0",
    "bcrypt": "^1.0.3",
    "classnames": "^2.2.5",
    "cloudinary": "^1.11.0",
    "commonmark": "^0.28.1",
    "handlebars": "^4.0.10",
    "juice": "^4.2.3",
    "lodash": "^4.17.5",
    "mailchimp-api-v3": "^1.8.0",
    "meteor-node-stubs": "^0.3.3",
    "moment": "^2.22.0",
    "prop-types": "^15.6.1",
    "query-string": "^6.0.0",
    "react": "^16.3.1",
    "react-async-script-loader": "^0.3.0",
    "react-avatar-editor": "^11.0.2",
    "react-burger-menu": "^2.4.2",
    "react-collapse": "^4.0.3",
    "react-day-picker": "^7.1.4",
    "react-dom": "^16.3.1",
    "react-images": "^0.5.17",
    "react-loading": "^1.0.4",
    "react-motion": "^0.5.2",
    "react-redux": "^5.0.7",
    "react-redux-loading-bar": "^4.0.2",
    "react-router-dom": "^4.2.2",
    "react-share": "^2.1.0",
    "react-slick": "^0.19.0",
    "react-stickynode": "^1.4.1",
    "react-stripe-elements": "^1.6.0",
    "react-typist": "^2.0.4",
    "redux": "^3.7.2",
    "semantic-ui-react": "0.77.1",
    "simpl-schema": "^1.4.3",
    "stripe": "^5.8.0",
    "uuid": "^3.2.1",
    "validator": "^9.4.1",
    "winston": "^2.4.1"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0"
  }
}
